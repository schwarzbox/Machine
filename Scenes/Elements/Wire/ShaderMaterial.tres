[sub_resource type="Shader" id=1]
code = "shader_type canvas_item;

uniform sampler2D textureMix;
uniform sampler2D colorTexture;

uniform vec2 scroll = vec2(0.1, 0.1);
uniform vec2 scrollMix = vec2(0.1, 0.1);

void fragment() {
	float textureColor = texture(TEXTURE, UV + scroll * TIME).r;
	float textureColorMix = texture(textureMix, UV + scrollMix * TIME).r;
	float energy = textureColor * textureColorMix;
	
	vec4 color = texture(colorTexture, vec2(energy));
	COLOR = color;
}"

